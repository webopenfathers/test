<view bindtap="onGoToDetail" class="container">
  <image class="poster" src="{{movie.images.large}}" mode="" />
  <text class="title">{{movie.original_title}}</text>
  <view class="rate-container">
    <l-rate disabled size="22" score="{{movie.rating.stars/10}}" />
    <text class="score">{{movie.rating.average}}</text>
  </view>
</view>